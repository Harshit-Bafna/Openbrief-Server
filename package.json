    "main": "server.js",
    "scripts": {
        "build": "npx tsc",
        "dev": "cross-env NODE_ENV=development nodemon src/server.ts",
        "start": "cross-env NODE_ENV=production node dist/server.js",
        "prepare": "husky",
        "lint:eslint": "eslint",
        "format:check": "prettier . --check",
        "format:fix": "prettier . --write",
        "db:migrate:dev": "cross-env NODE_ENV=development sequelize-cli db:migrate",
        "db:migrate:prod": "cross-env NODE_ENV=production sequelize-cli db:migrate",
        "db:migrate:undo:dev": "cross-env NODE_ENV=development sequelize-cli db:migrate:undo",
        "db:migrate:undo:prod": "cross-env NODE_ENV=production sequelize-cli db:migrate:undo"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@eslint/js": "^9.31.0",
        "@types/cookie-parser": "^1.4.9",
        "@types/express": "^5.0.3",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.1.0",
        "@types/nodemailer": "^6.4.17",
        "@types/sequelize": "^4.28.20",
        "@types/source-map-support": "^0.5.10",
        "eslint": "^9.31.0",
        "eslint-config-prettier": "^10.1.8",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.2",
        "nodemon": "^3.1.10",
        "prettier": "^3.6.2",
        "sequelize-cli": "^6.6.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.38.0"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.850.0",
        "@aws-sdk/s3-request-presigner": "^3.850.0",
        "@types/cors": "^2.8.19",
        "argon2": "^0.43.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "colorette": "^2.0.20",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.13",
        "dotenv-flow": "^4.1.0",
        "express": "^5.1.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.16.4",
